{
    "name": "a-frame for vscode",
    "displayName": "A-Frame IntelliSense",
    "description": "Autocompletion for A-Frame",
    "version": "0.0.1",
    "publisher": "pasalog",
    "author": "Çağatay Uslu <cagatayuslu@msn.com>",
    "engines": {
        "vscode": "^1.25.0"
    },
    "keywords": [
        "a-frame",
        "intellisense",
        "help"
    ],
    "categories": [
        "Other"
    ],
    "icon": "images/logo.jpg",
    "galleryBanner": {
        
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pasalog/vscode-a-frame-intellisense.git"
    },
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
